{
    "name": "bref/bref-laravel",
    "license": "MIT",
    "type": "project",
    "description": "Laravel integration for Bref",
    "homepage": "https://bref.sh/docs/frameworks/laravel.html",
    "keywords": ["bref", "serverless", "aws", "lambda", "faas"],
    "autoload": {
        "psr-4": {
            "Bref\\Bridge\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bref\\Test\\Bridge\\Laravel\\": "tests"
        }
    },
    "require": {
        "php": "^7.2.0",
        "mnapoli/bref": "~0.3.0",
        "laravel/framework": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "doctrine/coding-standard": "^5.0",
        "phpstan/phpstan": "^0.10.5"
    }
}
